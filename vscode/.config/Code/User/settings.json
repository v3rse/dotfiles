{
    "vim.enableNeovim": false,
    "keyboard.dispatch": "keyCode",
    "workbench.colorTheme": "Palenight Operator",
    "editor.minimap.enabled": false,
    "zenMode.hideActivityBar": false,
    "zenMode.fullScreen": false,
    "workbench.editor.showTabs": false,
    "javascript.validate.enable": false,
    "editor.tabSize": 2,
    "editor.fontFamily": "'Fira Code','Hack', 'Fura Code Regular Nerd Font Complete', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 15,
    "workbench.iconTheme": "vscode-great-icons",
    "workbench.sideBar.location": "right",
    "typescript.check.npmIsInstalled": false,
    "vim.disableExtension": false,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": true,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true,
        "suppressUpdateNotice": false,
        "suppressWelcomeNotice": true
    },
    "terminal.integrated.shell.linux": "/usr/bin/fish",
    "terminal.integrated.shell.osx": "/usr/local/bin/fish",
    "terminal.integrated.env.osx": {
        "GOOGLE_API_KEY": ""
    },
    "diffEditor.ignoreTrimWhitespace": true,
    "gitlens.keymap": "alternate",
    "editor.lineNumbers": "relative",
    "python.pythonPath": "/usr/local/bin/python3",
    "window.zoomLevel": 1,
    "git.autofetch": true,
    "breadcrumbs.enabled": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "wolf.logErrors": true,
    "wolf.pythonPath": "/usr/local/bin/python3",
    "window.menuBarVisibility": "toggle",
    "window.titleBarStyle": "native",
    "files.associations": {
        "**/.i3/config": "i3",
        "**/i3/config": "i3",
    },
    "editor.formatOnSave": true,
    "mocha-snippets.semicolon": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "cSpell.userWords": [
        "kudobuzz"
    ],
    "material-icon-theme.folders.theme": "specific",
    "liveshare.audio.joinCallBehavior": "accept",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    // "workbench.colorCustomizations": {
    //     "titleBar.activeBackground": "#82AAFF",
    //     "titleBar.activeForeground": "#292D3E",
    //     "titleBar.inactiveBackground": "#9CC4FF",
    //     "titleBar.inactiveForeground": "#434758"
    // }
}